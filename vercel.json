{
  "version": 2,
  "env": {
    "NODE_ENV": "production",
    "DEPLOYMENT_TRIGGER": "force-rebuild-2025-06-06"
  },
  "functions": {
    "app/api/assets/categorize/route.ts": {
      "runtime": "nodejs18.x"
    },
    "app/api/matrix/*/route.ts": {
      "runtime": "nodejs18.x"
    },
    "app/api/scenarios/*/route.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
} 