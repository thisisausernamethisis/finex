DATABASE_URL="postgres://user:pass@neon.db.../finex?pgbouncer=true&connect_timeout=15"
DIRECT_URL="postgres://user:pass@neon.db.../finex"
OPENAI_API_KEY="sk-..."
CLERK_SECRET_KEY="sk_..."
REDIS_URL="redis://127.0.0.1:6379"

# Rate limiting configuration (requests per minute)
# Production: 50, Test: 1, Mock: 100
RL_LIMIT="50"

# Alpha scoring strategy for hybrid search
# Options: heuristic (default), gpt (uses OpenAI for dynamic tuning)
FINEX_ALPHA_MODE="heuristic"

# OpenTelemetry configuration
ENABLE_OTEL="false"
OTEL_SERVICE_NAME="finex-api"
OTEL_ENDPOINT="http://localhost:4317/v1/traces"

# Search configuration
DYNAMIC_ALPHA="true"
DYNAMIC_ALPHA_GPT="false"
SEARCH_CACHE_TTL="3600"
SEARCH_DOMAIN_FILTER="true"

# Alpha-cache tuning (optional)
ALPHA_CACHE_MAX=50
ALPHA_CACHE_TTL_MS=300000
